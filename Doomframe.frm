	alias framework "echo FrameWork->[DoomFrame]"
	alias map(1) "map e1m1"
	alias map(2) "map e1m2"
	alias map(3) "map e1m3"
	alias map(4) "map e1m4"
	alias map(5) "map e1m5"
	alias map(6) "map e1m6"
	alias map(7) "map e1m7"
	alias map(8) "map e1m8"
	alias map(9) "map e1m9"
